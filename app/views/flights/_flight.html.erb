<div id="<%= dom_id flight %>">
  <p>
    <strong>Date:</strong>
    <%= flight.date %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= flight.name %>
  </p>

  <p>
    <strong>Departure:</strong>
    <%= flight.departure_id %>
  </p>

  <p>
    <strong>Arrival:</strong>
    <%= flight.arrival_id %>
  </p>

  <p>
    <strong>Vacancy:</strong>
    <%= flight.vacancy %>
  </p>

</div>
